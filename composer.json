{
  "name": "xpressengine-plugin/alice",
  "description": "This Package is Xpressengine Plugin - Alice Theme.",
  "keywords": [
    "xpressengine",
    "plugin",
	"theme",
	"alice",
    "material"
  ],
  "license": "LGPL-2.0",
  "version": "1.0.0",
  "type": "xpressengine-plugin",
  "authors": [
    {
      "name": "xpressengine",
      "email": "developers@xpressengine.com",
      "homepage": "http://www.xpressengine.com",
      "role": "Developer"
    }
  ],
  "extra": {
    "xpressengine": {
      "title": "Alice Theme",
      "component": {
        "theme/alice@main": {
          "class": "Xpressengine\\Plugins\\Alice\\Theme\\Main",
          "name": "Alice (for main)",
          "description": "The First Theme for XpressEngine3. use this to index page",
          "screenshot": "/plugins/alice/screenshots/main.png"
        },
        "theme/alice@sub": {
          "class": "Xpressengine\\Plugins\\Alice\\Theme\\Sub",
          "name": "Alice (for sub)",
          "description": "The First Theme for XpressEngine3. use this to sub page",
          "screenshot": "/plugins/alice/screenshots/sub.png"
        },
        "theme/alice@site": {
          "class": "Xpressengine\\Plugins\\Alice\\Theme\\Site",
          "name": "Alice (for site default)",
          "description": "The First Theme for XpressEngine3. use this to site theme",
          "screenshot": "/plugins/alice/screenshots/sub.png"
        }
      }
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://packagist.xpressengine.io/"
    }
  ],
  "require": {

  },
  "autoload": {
    "psr-4": {
      "Xpressengine\\Plugins\\Alice\\": "src/"
    }
  }
}
